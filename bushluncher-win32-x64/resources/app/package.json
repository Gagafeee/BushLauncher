{
    "name": "bush-launcher",
    "productionName": "Bush Launcher",
    "version": "0.0.2.0",
    "description": "An Minecraft Version, Mods Manager",
    "main": "main.js",
    "author": "Gagafeee",
    "license": "MIT",
    "directory": {
        "output": "out"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "dist": "electron-builder"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.67",
        "@electron-forge/maker-deb": "^6.0.0-beta.67",
        "@electron-forge/maker-rpm": "^6.0.0-beta.67",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
        "@electron-forge/maker-zip": "^6.0.0-beta.67",
        "electron": "^21.2.0",
        "electron-builder": "^23.6.0"
    },
    "dependencies": {
        "appdata-path": "^1.0.0",
        "browserify": "^17.0.0",
        "browserify-fs": "^1.0.0",
        "electon": "^0.0.1-security",
        "electron-squirrel-startup": "^1.0.0",
        "electron-winstaller": "^5.1.0",
        "minecraft-launcher-core": "^3.16.16",
        "msmc": "^3.1.3",
        "node-fetch": "^3.2.10",
        "original-fs": "^1.2.0",
        "os": "^0.1.2",
        "robloach-datfile": "^2.3.0"
    },
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "minecraft_manager"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "build": {
        "appId": "com.gagafeee.bushluncher",
        "target": "NSIS",
        "nsis": {
            "allowToChangeInstallationDirectory": true,
            "oneClick": false
        }
    }
}
